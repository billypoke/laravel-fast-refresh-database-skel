{
    "name": "plannr/laravel-fast-refresh-database",
    "description": "Refresh your database faster than you've ever seen before ðŸš€",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam CarrÃ©",
            "email": "29132017+Sammyjo20@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.13.0",
        "illuminate/contracts": "^9.0",
        "symfony/process": "^5.0|^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Plannr\\Laravel\\FastRefreshDatabase\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ],
        "test": [
            "./vendor/bin/pest"
        ],
        "fix-code": [
            "./vendor/bin/php-cs-fixer fix"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Plannr\\Laravel\\FastRefreshDatabase\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
